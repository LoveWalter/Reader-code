package com.zwt.myapp.bean;

/**
 * 
 * 记录书的地址及导入状态
 * 
 * @author
 */
public class BookBean {
	private String owen;// 书籍路径
	private int local;// 导入状态

	// 地址
	public String getOwen() {
		return owen;
	}

	public void setOwen(String owen) {
		this.owen = owen;
	}

	// 导入状态
	public int getLocal() {
		return local;
	}

	public void setLocal(int local) {
		this.local = local;
	}

	public BookBean(String owen, int local) {
		super();
		this.owen = owen;
		this.local = local;
	}

}
